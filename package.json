{
  "name": "PartiuPDV",
  "version": "1.0.0",
  "description": "A Partiu Point of Sale application",
  "main": "src/main.js",
  "homepage": "http://www.partiuvantagens.com.br",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . PartiuPDV --platform win32 --arch x64 --out dist/",
    "setup": "electron-installer-windows --src dist/PartiuPDV-win32-x64/ --config config.json"
  },
  "keywords": [
    "Partiu Vantagens",
    "POS",
    "pos"
  ],
  "author": "Arkady Zalkowitsch",
  "license": "MIT",
  "devDependencies": {
    "electron": "^1.4.1",
    "electron-installer-windows": "^0.2.0",
    "electron-winstaller": "^2.3.4"
  },
  "dependencies": {
    "electron-installer-windows": "*",
    "electron-packager": "*",
    "electron-prebuilt": "*"
  }
}